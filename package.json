{
  "name": "nunpress",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "browserify": "browserify _/js/app.js -o dist/js/app.bundle.js -t [ babelify --presets [ es2015 ] ]",
    "watchify": "watchify -d _/js/app.js -o src/js/app.bundle.js -t [ babelify --presets [ es2015 ] ]",
    "sass": "node-sass _/scss/main.scss | postcss --use autoprefixer --autoprefixer.browsers \"> 5%\" -o src/css/main.min.css",
    "browserSync": "browser-sync start --ws --no-ui --proxy http://localhost:3000 --port 3001 --reload-delay 3000 --files \"src/**/*.*\" ",
    "watch:server": "nodemon -L -e js --watch _/express --watch bin",
    "watch:sass": "nodemon -L -e scss -w _/scss -x \"npm run sass\"",
    "dev:server": "concurrent 'npm run watch:server' 'npm run watch:sass' 'npm run watchify' 'npm run browserSync'"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "compression": "^1.6.0",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "helmet": "^1.0.1",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "concurrently": "^1.0.0",
    "node-sass": "^3.4.2",
    "nodemon": "^1.8.1",
    "postcss-cli": "^2.5.0",
    "watchify": "^3.7.0"
  }
}
